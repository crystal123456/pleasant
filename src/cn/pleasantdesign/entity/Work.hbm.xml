<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="cn.pleasantdesign.entity">
  <class name="Work" table="pleasant_work">
  		<id name="id" type="java.lang.Integer">
  			<column name="id"></column>
			<generator class="native"></generator>
		</id>

		<property name="workName" column="work_name"></property>
		<property name="workPicPath1" column="work_picPath1"></property>
		<property name="workPicPath2" column="work_picPath2"></property>
		<property name="workPicPath3" column="work_picPath3"></property>
		<property name="workPicPath4" column="work_picPath4"></property>
		<property name="workPicPath5" column="work_picPath5"></property>
  </class>
</hibernate-mapping>